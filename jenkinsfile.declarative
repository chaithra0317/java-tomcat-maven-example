pipeline {
    agent any
	stages {
		stage("checkout") {
			steps {
				git clone https://github.com/chaithra0317/java-tomcat-maven-example.git
			}
		}
		stage("build") {
			steps {
				sh 'mvn clean package'
			}
		}
		stage("deploy") {
			steps {
				deploy adapters: [tomcat9(credentialsId: 'tomcat-server', path: '', url: 'http://13.232.252.5:8080/')], contextPath: null, war: '**/*.war'
				}
			}
		}
}	
			
	
